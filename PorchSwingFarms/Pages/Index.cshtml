@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>


<div class="container" style="margin-top: 30px;">
    <div class="row justify-content-center" id="fileinputrow">
        <div class="col-md-4 ">
            <input type="file" id="fileToUpload" class="form-control">
        </div>
        <div class="col-md-2">
            <button type="button" class="btn btn-info btn-sm" id="btnUploadFile">Upload File</button>
            <button type="button" class="btn btn-info btn-sm" id="btnRemoveFile">Remove File</button>
        </div>
    </div>
        <div class="table-responsive col-md-12 csv-table" style="margin-top: 20px;">
            <table class="table table-bordered table-hover table-striped"></table>
        </div>
</div>
